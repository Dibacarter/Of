<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Website Updates</title>
</head>
<body>
    <h1>Latest Updates</h1>
    <ul>
        <% updates.forEach(function(update) { %>
            <li><strong><%= update.title %></strong>: <%= update.content %></li>
        <% }); %>
    </ul>
</body>
</html>
